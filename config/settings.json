{
  "certPath": "/certs",
  "enableHttps": false,
  "httpsPort": 4443,
  "httpsCertPath": null,
  "httpsKeyPath": null,
  "openSSLPath": "openssl",
  "caValidityPeriod": {
      "rootCA": 3650,
      "intermediateCA": 1825,
      "standard": 90
  },
  "autoRenewByDefault": true,
  "renewDaysBeforeExpiry": 30,
  "logLevel": "info",
  "jsonOutput": false,
  "enableCertificateBackups": true,
  "keepBackupsForever": true,
  "backupRetention": 90,
  "signStandardCertsWithCA": false,
  "enableAutoRenewalJob": true,
  "renewalSchedule": "0 0 * * *",
  "enableFileWatch": true
}